local function SIBJL_fake_script() -- VaxFrame.MainScript 
	local script = Instance.new('LocalScript', VaxFrame)

	script.Parent.Menu.Main.Scroll.Home.MouseButton1Click:Connect(function()
		script.Parent.Typa.Home.Visible = true
		script.Parent.Typa.MainScript.Visible = false
		script.Parent.Typa.Check.Visible = false
		script.Parent.Typa.Credits.Visible = false
	end)
	
	script.Parent.Menu.Main.Scroll["Main Script"].MouseButton1Click:Connect(function()
		script.Parent.Typa.Home.Visible = false
		script.Parent.Typa.MainScript.Visible = true
		script.Parent.Typa.Check.Visible = false
		script.Parent.Typa.Credits.Visible = false
	end)
	
	script.Parent.Menu.Main.Scroll.Checker.MouseButton1Click:Connect(function()
		script.Parent.Typa.Check.Visible = true
		script.Parent.Typa.MainScript.Visible = false
		script.Parent.Typa.Home.Visible = false
		script.Parent.Typa.Credits.Visible = false
	end)
	
	script.Parent.Menu.Main.Scroll.Credits.MouseButton1Click:Connect(function()
		script.Parent.Typa.Check.Visible = false
		script.Parent.Typa.MainScript.Visible = false
		script.Parent.Typa.Home.Visible = false
		script.Parent.Typa.Credits.Visible = true
	end)
end
coroutine.wrap(SIBJL_fake_script)()
