local function JRUJ_fake_script() -- avatar.LocalScript 
	local script = Instance.new('LocalScript', avatar)

	
	
	local characterImageFormat='http://www.roblox.com/Thumbs/Avatar.ashx?x=352&y=352&format=png&userid=%d';
	
